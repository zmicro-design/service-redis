export SERVICE_REDIS_NAME=redis

export SERVICE_REDIS_VERSION=v7

export SERVICE_REDIS_HOST=${SERVICE_REDIS_HOST:-0.0.0.0}
export SERVICE_REDIS_PORT=${SERVICE_REDIS_PORT:-6379}

export SERVICE_REDIS_SECRET=${SERVICE_REDIS_SECRET}

export SERVICE_REDIS_RESOURCE_LIMIT_CPU=${SERVICE_REDIS_RESOURCE_LIMIT_CPU:-4}
export SERVICE_REDIS_RESOURCE_LIMIT_MEM=${SERVICE_REDIS_RESOURCE_LIMIT_MEM:-8192m}
